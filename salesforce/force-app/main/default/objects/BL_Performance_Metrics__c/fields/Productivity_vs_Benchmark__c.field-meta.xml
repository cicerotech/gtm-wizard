<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Productivity_vs_Benchmark__c</fullName>
    <description>Percentage of avg monthly productivity vs $350K benchmark. 100% = at benchmark.</description>
    <externalId>false</externalId>
    <formula>IF(Avg_Monthly_Productivity__c != null &amp;&amp; Avg_Monthly_Productivity__c > 0,
  (Avg_Monthly_Productivity__c / 350000) * 100,
  0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Productivity vs Benchmark (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>

