<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ramp_Progress__c</fullName>
    <description>Progress toward $2M ramp milestone as percentage. 100% = fully ramped.</description>
    <externalId>false</externalId>
    <formula>IF(Ramp_Milestone__c != null &amp;&amp; Ramp_Milestone__c > 0,
  MIN((Total_Closed_Won_ACV__c / Ramp_Milestone__c) * 100, 100),
  0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Ramp Progress (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>

