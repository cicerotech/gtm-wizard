<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quota_Attainment__c</fullName>
    <description>YTD ACV as percentage of Annual Quota</description>
    <externalId>false</externalId>
    <formula>IF(Annual_Quota__c &gt; 0, Fiscal_YTD_ACV__c / Annual_Quota__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Quota Attainment</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>

