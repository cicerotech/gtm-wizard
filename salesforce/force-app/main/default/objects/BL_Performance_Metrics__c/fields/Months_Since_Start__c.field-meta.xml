<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Months_Since_Start__c</fullName>
    <description>Number of months since BL start date. Used for ramp tracking.</description>
    <externalId>false</externalId>
    <formula>IF(Start_Date__c != null, (TODAY() - Start_Date__c) / 30.44, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Months Since Start</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
</CustomField>

