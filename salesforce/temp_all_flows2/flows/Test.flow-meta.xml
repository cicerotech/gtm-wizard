    <environments>Default</environments>
    <formulas>
        <name>DefaultCloseOrTargetSign</name>
        <dataType>Date</dataType>
        <expression>IF(ISBLANK($Record.Target_LOI_Date__c), TODAY() + 150, $Record.Target_LOI_Date__c)</expression>
    </formulas>
    <formulas>
        <name>OppName</name>
        <dataType>String</dataType>
        <expression>IF(
ISBLANK($Record.Name),
/* If you fetched Account in your Get Records step */
IF(NOT(ISBLANK({!account.Name})),
{!account.Name} &amp; &quot; - &quot; &amp; {!StageLabel},
&quot;Untitled - &quot; &amp; {!StageLabel}
),
$Record.Name
)</expression>
    </formulas>
    <formulas>
        <name>Probability</name>
        <dataType>Number</dataType>
        <expression>IF(ISBLANK($Record.Probability), 10, $Record.Probability)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>StageLabel</name>
        <dataType>String</dataType>
        <expression>CASE(TEXT($Record.StageName),
&quot;Stage 0 - Qualifying&quot;,&quot;Qualifying&quot;,
&quot;Stage 1 - Discovery&quot;,&quot;Discovery&quot;,
&quot;Stage 2 - SQO&quot;,&quot;SQO&quot;,
TEXT($Record.StageName)
)</expression>
    </formulas>
    <interviewLabel>Test {!$Flow.CurrentDateTime}</interviewLabel>
