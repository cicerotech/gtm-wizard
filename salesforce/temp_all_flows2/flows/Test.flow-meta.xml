        <label>ACV Check</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Assignment_Values</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>use defaults</defaultConnectorLabel>
        <rules>
            <name>User_Provided_ACV</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ACV__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Close_Date</targetReference>
            </connector>
            <label>User_Provided_ACV</label>
        </rules>
    </decisions>
    <decisions>
        <name>Close_Date</name>
